            }
        }
        stage('DEPLOY') {
            steps {
                echo 'deploy initiate ...'
            }
        }
        stage('TEST') {
            steps {
                echo 'testing initiate ...'
            }
        }
        stage('RELEASE') {
            steps {
                echo 'release initiate ...'
            }
        }
    }
}
